{
  "name": "capacitor-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "credentials": "node scripts/credentials.js",
    "credentials-force": "node scripts/credentials.js --force-bundle",
    "prestart": "yarn credentials",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "android": "yarn build && yarn cap run android --target Pixel_9_Pro",
    "ios": "yarn build && yarn cap run ios --target 0D3D6293-9DDD-4420-B1EA-ACF32640F09E",
    "dev:ios": "yarn cap run ios --live-reload --no-sync --host 192.168.1.76 --port 5173 --target 0D3D6293-9DDD-4420-B1EA-ACF32640F09E",
    "dev:android": "yarn cap run android --live-reload --no-sync --host 192.168.1.76 --port 5173 --target Pixel_9_Pro",
    "link-assets": "ruby scripts/copy-adapty-assets.rb",
    "capacitor:sync:before": "ruby scripts/copy-adapty-assets.rb"
  },
  "dependencies": {
    "@adapty/capacitor": "file:..",
    "@capacitor/android": "7.0.0",
    "@capacitor/core": "latest",
    "@capacitor/ios": "7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "prompt-sync": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.2",
    "xcode": "^3.0.1"
  },
  "author": "",
  "license": "ISC"
}
