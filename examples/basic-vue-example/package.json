{
  "name": "adapty-basic-vue-example",
  "version": "1.0.0",
  "description": "Adapty capacitor vue example",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile",
    "adapty",
    "vue"
  ],
  "scripts": {
    "credentials": "node ../../scripts/credentials.mjs --skip-onboarding",
    "credentials-force": "node ../../scripts/credentials.mjs --force-bundle --skip-onboarding",
    "postinstall": "yarn credentials",
    "start": "vite",
    "build": "vite build",
    "tsc": "tsc --noEmit",
    "update-native-modules": "cd ./ios/App && pod update Adapty AdaptyUI AdaptyPlugin && yarn run update-cap-config",
    "update-cap-config": "yarn cap copy && yarn cap sync",
    "android": "yarn build && yarn cap run android",
    "ios": "yarn build && yarn cap run ios"
  },
  "dependencies": {
    "@adapty/capacitor": "3.16.0-beta.0",
    "@capacitor/android": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.2",
    "vue-tsc": "^3.1.0"
  },
  "author": "",
  "license": "MIT"
}
